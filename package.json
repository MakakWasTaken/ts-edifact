{
  "name": "@makakwastaken/ts-edifact",
  "description": "Edifact parser library",
  "version": "0.2.10",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": ["edi", "edifact", "parser"],
  "publisher": "Roman Vottner",
  "readme": "README.md",
  "author": {
    "name": "Roman Vottner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MakakWasTaken/ts-edifact.git"
  },
  "homepage": "https://github.com/MakakWasTaken/ts-edifact/README.md",
  "bugs": {
    "url": "https://github.com/MakakWasTaken/ts-edifact/-/issues",
    "email": "markus@unknown-studios.com"
  },
  "contributors": [
    {
      "name": "Markus Moltke",
      "email": "markus@unknown-studios.com",
      "url": "https://unknown-studios.com"
    }
  ],
  "lint-staged": {
    "*.ts": ["biome lint --write", "biome format --write"]
  },
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "lint": "biome format --write --no-errors-on-unmatched . && biome lint --write .",
    "build": "tsc -b",
    "check": "npm run check:tsc",
    "check:tsc": "tsc -p . --noEmit",
    "test": "jest spec",
    "coverage": "yarn test --coverage",
    "lint:staged": "lint-staged",
    "prepack": "yarn lint && yarn build",
    "prepare": "husky || true"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@initics/tsm": "^1.0.2",
    "@swc/jest": "^0.2.36",
    "@types/events": "3.0.3",
    "@types/htmlparser2": "^3.10.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.15",
    "domhandler": "^5.0.3",
    "events": "^3.3.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.9",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0"
  },
  "dependencies": {
    "@swc/core": "^1.7.11",
    "axios": "^1.7.4",
    "htmlparser2": "^9.1.0"
  },
  "files": ["build/**/*"],
  "packageManager": "yarn@4.4.0"
}
