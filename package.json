{
  "name": "@makakwastaken/ts-edifact",
  "description": "Edifact parser library",
  "version": "0.2.13",
  "type": "module",
  "main": "build/cjs/src/index.js",
  "module": "build/esm/src/index.js",
  "types": "build/esm/src/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "edi",
    "edifact",
    "parser"
  ],
  "readme": "README.md",
  "author": {
    "name": "Roman Vottner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MakakWasTaken/ts-edifact.git"
  },
  "homepage": "https://github.com/MakakWasTaken/ts-edifact/README.md",
  "bugs": {
    "url": "https://github.com/MakakWasTaken/ts-edifact/-/issues",
    "email": "markus@unknown-studios.com"
  },
  "contributors": [
    {
      "name": "Markus Moltke",
      "email": "markus@unknown-studios.com",
      "url": "https://unknown-studios.com"
    }
  ],
  "lint-staged": {
    "*.ts": [
      "biome lint --write",
      "biome format --write"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "lint": "biome format --write --no-errors-on-unmatched . && biome lint --write .",
    "check": "npm run check:tsc",
    "check:tsc": "tsc -p . --noEmit",
    "test": "jest spec",
    "coverage": "yarn test --coverage",
    "lint:staged": "lint-staged",
    "prepack": "yarn lint && yarn build",
    "prepare": "husky || true",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@initics/tsm": "^1.0.2",
    "@swc/jest": "^0.2.37",
    "@types/events": "3.0.3",
    "@types/htmlparser2": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.2",
    "domhandler": "^5.0.3",
    "events": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.4",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0"
  },
  "dependencies": {
    "@swc/core": "^1.11.16",
    "axios": "^1.8.4",
    "htmlparser2": "^10.0.0"
  },
  "files": [
    "build/**/*"
  ],
  "packageManager": "yarn@4.8.1"
}
